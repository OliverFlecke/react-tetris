{
  "name": "tetris",
  "version": "0.1.0",
  "private": true,
  "author": "Oliver Fleckenstein",
  "homepage": "http://OliverFlecke.github.io/tetris",
  "dependencies": {
    "@types/jest": "24.0.6",
    "@types/node": "11.9.4",
    "@types/react": "16.8.3",
    "@types/react-dom": "16.8.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-scripts": "2.1.5",
    "typescript": "3.3.3"
  },
  "scripts": {
    "lint-ts": "npx tslint --project tsconfig.json",
    "lint-prettier": "npx prettier --config .prettierrc \"./**/*.{js,jsx,ts,tsx,json,scss,md}\" --write",
    "lint": "npm run lint-ts && npm run lint-prettier",
    "build-scss": "npx tcm src -p **/*.scss",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "node-sass": "^4.11.0",
    "prettier": "^1.16.4",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^3.6.0",
    "typed-css-modules": "^0.3.7",
    "typings-for-css-modules-loader": "^1.7.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
